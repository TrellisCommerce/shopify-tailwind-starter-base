    "2af0e9d7-f2a8-434b-b346-ee8969847698": {
      "type": "full-width-banner",
      "settings": {
        "background_image": "shopify:\/\/shop_images\/m0k13_photorealistic_image_of_a_happy_dog_in_front_mountains_ea_0d2b07ee-b9a6-421c-8ef0-6a94b7499025.png",
        "content": "<h1><strong>Jede Mahlzeit zählt: <br\/>Der Uncle Rusty's Mix<\/strong><\/h1><p><em>Stelle das ideale Paket zusammen: <\/em><br\/>3x helles Fleisch, 3x rotes Fleisch, 1x Fisch.  Der Schlüssel für eine rundum gesunde Ernährung deines Hundes. Einfach, effektiv, ernährungsreich!<\/p>",
        "button_text": "Mix & Match: Dein Menü",
        "button_link": "shopify:\/\/pages\/build-a-box",
        "button2_text": "Ernährungsberatung",
        "button2_link": "shopify:\/\/pages\/questionaire",
        "newsletter": "false"
      }
    },
